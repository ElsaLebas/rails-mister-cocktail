<div class="tom-container home">
  <!-- <h1>Cocktail and Dreams ...</h1> -->

  <div id="tom">
    <%= cl_image_tag("gufotxpwqllczdfjappk.jpg")%>
  </div>

  <div class = "cards">
    <% @cocktail.each do |cocktail| %>
      <div class = "card">
        <%= link_to "#{cocktail.name}", cocktail_path(cocktail) %>
      </div>
    <% end %>

    <div class="card">
     <%= link_to "Create new Cocktail", new_cocktail_path  %>
    </div>

  </div>
</div>
